<% include layout/_header %>
  <div class="container">
    <div class="row">

      <div class="col-sm-12">
        <h1><%= title %> <%= author %></h1>
        <ul class="nav nav-pills nav-stacked">
          <% for(let post of posts){ %>
            <li>
              <a href="/post/<%= posts.id %>">
                <%= post.postName %>
              </a>
            </li>
            <% } %>
        </ul>

      </div>
    </div>
  </div>
  <% include layout/_footer %>
